    "ulid": "^2.3.0",
    "vk-io": "^4.8.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.1",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "28.1.8",
    "@types/node": "^18.15.13",
